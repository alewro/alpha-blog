<h1>Articles</h1>

<table>
  <thead>
    <th>Title</th>
    <th>Description</th>
    <th>Action</th>
  </thead>
  <body>
    <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.description %></td>
      <td><%= link_to "show article", @article %></td>
    </tr>
    <% end %>
  </body>
</table>
